{
  "name": "kairos",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://gaddev.github.io/kairos/",
  "dependencies": {
    "cross-fetch": "^3.0.4",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-event-listener": "^0.6.6",
    "react-redux": "^7.1.1",
    "react-scripts": "3.2.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "autoprefixer": "^9.7.0",
    "babel-jest": "^24.9.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "classnames": "^2.2.6",
    "cross-env": "^6.0.3",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "fetch-mock": "^8.0.0-alpha.14",
    "gh-pages": "^2.1.1",
    "husky": "^4.0.0-beta.5",
    "jest": "^24.9.0",
    "jest-enzyme": "^7.1.1",
    "lint-staged": "^7.0.5",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.1.2",
    "postcss-nested-ancestors": "^2.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.11.0",
    "redux-devtools": "^3.5.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3",
    "stylelint": "^11.1.1",
    "stylelint-config-css-modules": "^1.5.0",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-order": "^3.1.1",
    "stylelint-prettier": "^1.1.1",
    "svgo": "^1.3.0"
  },
  "scripts": {
    "build:css": "postcss src/styles/main.css -o src/index.css",
    "watch:css": "postcss src/styles/main.css -o src/index.css -w",
    "start": "react-scripts start",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d build",
    "build": "react-scripts build",
    "lint": "eslint --ext .js,.jsx .",
    "lint:fix": "eslint --ext .js,.jsx . --fix",
    "eject": "react-scripts eject",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "set CI=true && jest test --coverage"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "lint-staged": {
    "*.css": [
      "prettier --write",
      "stylelint --fix",
      "git add"
    ],
    "*.{js,jsx}": [
      "eslint --ext .js,.jsx",
      "prettier --write",
      "git add",
      "eslint . --fix"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/*/*/*.{js,jsx}",
      "**/src/*/*.{js,jsx}",
      "**/src/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/serviceWorker.js",
      "**/index.js",
      "!**/src/index.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 95,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/assetsTransformer.js"
    }
  }
}
